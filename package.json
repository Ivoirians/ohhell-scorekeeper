{
  "name": "ohhellscorekeeper",
  "version": "1.0.0",
  "description": "Scorekeeping app for the Zazzle games lunch crew",
  "main": "index.js",
  "scripts": {
    "setup": "(npm list firebase -g || npm install firebase -g) && (npm list firebase-tools -g || npm install firebase-tools -g) && (npm list webpack-dev-server -g || npm install webpack-dev-server -g) && (npm list webpack -g || npm install webpack -g)",
    "dev": "webpack-dev-server --config webpack/webpack-dev.config.js --content-base public/ --watch --hot",
    "build": "webpack --config webpack/webpack-prod.config.js --optimize-minimize --define process.env.NODE_ENV='production'",
    "deploy": "npm run build && firebase deploy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Kevin Shen",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ivoirians/ohhell-scorekeeper.git"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "enum": "^2.4.0",
    "firebase": "^3.6.9",
    "moment": "^2.18.1",
    "react": "^15.4.2",
    "react-datepicker": "^0.43.0",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-loader": "^7.1.5",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "webpack": "^2.2.1",
    "webpack-cli": "^3.3.2"
  }
}
